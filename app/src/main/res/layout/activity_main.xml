<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.ice.tiptext.widget.TipTextView
        android:layout_marginTop="20dp"
        android:id="@+id/tv1"
        android:padding="2dp"
        android:background="#ff0000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:left_text="的接京东方哈伦裤第三方数成本据蓝思科技登录时口"
        app:right_text="标签"
        app:left_text_size="20dp"
        app:right_text_size="14dp"
        app:right_text_color="#00ff00"
        app:right_text_rect_color="#00ff00"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.ice.tiptext.widget.TipTextView
        android:layout_marginTop="20dp"
        android:id="@+id/tv2"
        android:padding="2dp"
        android:background="#ff0000"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_text="的接京东方哈伦裤第三方数成本据蓝思科技登录时口"
        app:right_text="标签"
        app:left_text_size="20dp"
        app:right_text_size="14dp"
        app:right_text_color="#00ff00"
        app:right_text_rect_color="#00ff00"
        app:layout_constraintTop_toBottomOf="@id/tv1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <com.ice.tiptext.widget.TipTextView
        android:layout_marginTop="20dp"
        android:id="@+id/tv3"
        android:padding="2dp"
        android:background="#ff0000"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        app:left_text="的接京东方哈伦裤第三方数成本据蓝思科技登录时口"
        app:right_text="标签"
        app:left_text_size="20dp"
        app:right_text_size="14dp"
        app:right_text_color="#00ff00"
        app:right_text_rect_color="#00ff00"
        app:layout_constraintTop_toBottomOf="@id/tv2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <com.ice.tiptext.widget.TipTextView
        android:layout_marginTop="20dp"
        android:id="@+id/tv4"
        android:padding="2dp"
        android:background="#ff0000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:left_text="的接京东方"
        app:right_text="标签"
        app:left_text_size="20dp"
        app:right_text_size="14dp"
        app:right_text_color="#00ff00"
        app:right_text_rect_color="#00ff00"
        app:layout_constraintTop_toBottomOf="@id/tv3"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <com.ice.tiptext.widget.TipTextView
        android:layout_marginTop="20dp"
        android:id="@+id/tv5"
        android:padding="2dp"
        android:background="#ff0000"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_text="的接京东方"
        app:right_text="标签"
        app:left_text_size="20dp"
        app:right_text_size="14dp"
        app:right_text_color="#00ff00"
        app:right_text_rect_color="#00ff00"
        app:layout_constraintTop_toBottomOf="@id/tv4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <com.ice.tiptext.widget.TipTextView
        android:layout_marginTop="20dp"
        android:id="@+id/tv6"
        android:padding="2dp"
        android:background="#ff0000"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        app:left_text="的接京东方"
        app:right_text="标签"
        app:left_text_size="20dp"
        app:right_text_size="14dp"
        app:right_text_color="#00ff00"
        app:right_text_rect_color="#00ff00"
        app:layout_constraintTop_toBottomOf="@id/tv5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <com.ice.tiptext.widget.CommonSwitch
        android:id="@+id/cs"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv6"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />

    <com.ice.tiptext.widget.CommonButton
        android:id="@+id/cb"
        android:layout_marginTop="20dp"
        android:layout_width="50dp"
        android:layout_height="20dp"
        app:layout_constraintTop_toBottomOf="@id/cs"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>